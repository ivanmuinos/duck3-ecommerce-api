{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "id_product": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "url": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "price": {
      "required": true,
      "unique": true,
      "type": "decimal"
    },
    "priceText": {
      "type": "string"
    },
    "rating": {
      "type": "integer"
    },
    "description": {
      "type": "richtext"
    },
    "specs": {
      "type": "richtext"
    },
    "thumbnail": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::thumbnail.thumbnail"
    },
    "media": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::media.media"
    },
    "sizes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::size.size"
    },
    "colors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::color.color"
    }
  }
}
